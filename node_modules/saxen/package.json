{
  "name": "saxen",
  "description": "A tiny, super fast, namespace aware sax-style XML parser written in plain JavaScript",
  "scripts": {
    "all": "run-s lint test-coverage test-perf",
    "lint": "eslint .",
    "pretest": "rollup -c",
    "test": "mocha test/index.js",
    "test-coverage": "NO_PERF=1 nyc --check-coverage --branches 95 -r text -r html npm test",
    "test-perf": "node test/perf"
  },
  "keywords": [
    "xml",
    "sax",
    "parser",
    "pure"
  ],
  "version": "8.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "bugs": {
    "url": "https://github.com/nikku/saxen/issues"
  },
  "author": {
    "name": "Vopilovskiy Konstantin",
    "email": "flash.vkv@gmail.com",
    "url": "http://vflash.ru"
  },
  "contributors": [
    {
      "name": "Nico Rehwaldt",
      "url": "https://github.com/nikku"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikku/saxen"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-plugin-bpmn-io": "^0.5.2",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.3.0",
    "rollup": "^0.57.1",
    "rollup-plugin-uglify": "^3.0.0",
    "table": "^4.0.2"
  }
}
